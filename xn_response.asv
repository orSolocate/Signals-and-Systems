% X[n] frequancy response
n=-100:1:100;
%x=2*cos((3/10)*pi*n)*cos((1/10)*pi*n);
x=2*cos((1/10)*pi*n).*cos((3/10)*pi*n);
plot(n,abs(x))
ax=gca;
ax.YLim= [-0.1,2.1];
[h,w] = freqz(x,1);
plot(w,abs(x))
ax = gca;
ax.YLim = [-1 2];
ax.XTick = 0:.05:2;
xlabel('Normalized Frequency (\times\pi rad/sample)')
ylabel('Magnitude')