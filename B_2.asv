% X[n] frequancy response
n=-100:1:100;
x=2*cos((1/10)*pi*n).*cos((3/10)*pi*n);
w = 2*pi * (0:(N-1)) / N;
w2 = fftshift(w);
w3 = unwrap(w2 - 2*pi);
figure
subplot(2,1,1)
plot(n,abs(x))
xlabel('n [discrete time]');
ylabel('x[n]');

%DTFT of x[n]
subplot(2,1,2)
N=201
w = 2*pi * (0:(N-1)) / N;
w2 = fftshift(w);
w3 = unwrap(w2 - 2*pi);
plot(w3/pi,abs(fftshift(x)))
xlabel('w [rad]');
ylabel('|X(e^jw)|');